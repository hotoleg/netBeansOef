<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>SluitingsDagen</welcome-file>
    </welcome-file-list>
    <display-name>Frituur Frida</display-name>
            <context-param> 
                <param-name>straat</param-name> 
                <param-value>Somerstraat</param-value> 
            </context-param>
            <context-param>
                <param-name>huisnummer</param-name>
                <param-value>22</param-value>
            </context-param>
            <context-param>
                <param-name>postcode</param-name>
                <param-value>2060</param-value>
            </context-param>
            <context-param>
                <param-name>gemeente</param-name>
                <param-value>Antwerpen</param-value>
            </context-param>
</web-app>
